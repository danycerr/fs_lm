# Solve a singular or non-singular problem
# default parameter
singularity = true        

# mesh name 
mesh_name=mesh/cube.msh
# In what dimension
#now red from the mesh used if mesh is generated
dimension = 2 
# division of the square generated by libmehs
ndiv =40

# With what elements ("tensor", "simplex")
element_type = tensor

# minimum approximation order
p_order =2

# Mesh refinement type: h, p, or hp
refinement_type = h 

# Number of uniform refinement steps
uniform_h_r_steps = 1

# Maximum number of adaptive refinement steps
# not used
max_adaptive_r_steps = 4

# Percentage of elements to refine at each step
# not used
refine_fraction = 0.8

# Percentage of elements to coarsen at each step
# not used
coarsen_fraction = 0.1

# Maximux h refinement level for an element
# not used
max_h_level = 9

# Penalty term, it ensures interior penalty formulation stability
ip_penalty = 20.
# interior penalty for interface
ip_penalty_int = 20.
# iterior penalty for adjoint problem not used anymore
ip_penalty_adj= 2000.


#friction model
# friction limit
mulim=-0.4
# control formulation weight of the beta 
gamma=0.1
# update percentage of the evaluated beta
alpha=0.000002
# maximum number of iterations control algorthm
nmaxit=50
# print solution after every printstep
printstep=2
